<Project>

    <PropertyGroup>
        <ConquerorVersion>0.1.0-beta.18</ConquerorVersion>
        <ConquerorAssemblyVersion>0.1.0.18</ConquerorAssemblyVersion>

        <LangVersion Condition="'$(TargetFramework)' == 'net6.0'">10</LangVersion>
        <LangVersion Condition="'$(TargetFramework)' == 'net7.0'">11</LangVersion>

        <Nullable>enable</Nullable>
